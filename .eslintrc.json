    "env": {
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "vue/require-default-prop": "off",
        "vue/multi-word-component-names": 0
    }
